class p06csx{

 void main(){
  print("Testing program p06csx\n");
 
  if (true)
      print("if then stmt works\n");

  if (false)
      print("if then stmt DOESN'T work\n");

  if (true)
       print("if then else stmt works\n");
  else print("if then else stmt DOESN'T work\n");

